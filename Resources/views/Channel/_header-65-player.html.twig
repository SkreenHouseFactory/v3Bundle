<div class="row">
  <h1 class="lato"> <img src="http://mskstatic.com/medias/ftp/chaines/mediacenter-inconnus/player.png" alt="integrale">L’Intégrale des Inconnus</h1>
  <h2 class="lato subtitleex">Tous les sketches cultes en streaming gratuit !</h2>
  {% for slider in selection_sketches | to_array %}
  {% if loop.first %}
  {% for program in slider.programs | to_array %}
  {% if loop.first %}
  <div class="col-xs-11 player-block">
    <iframe frameborder="0"  scrolling="no" width="100%" height="370" src="{{ program.player.player.url }}"></iframe>
  </div>
  <div class="col-xs-5 myskreen-block">
    <div>
      <span>
        <img src="{{ asset('bundles/skreenhousefactoryv3/images/logo-footer.png') }}" alt="myskreen">
      </span>
      <a href="http://www.myskreen.com/les-inconnus/">
        <img src="http://mskstatic.com/275/160/c/medias/photos/channel/1-les-inconnus-s.jpg?2013112216" alt="">
      </a>
      <p><a href="http://www.myskreen.com/les-inconnus/">Suivez <strong>les Inconnus</strong> sur myskreen et recevez gratuitement les prochaines vidéos en avant-première et les images exclusives du film Les Trois Frères, le retour !</a></p>
   <div class="suivre-media">
     {% if '/partners/' in app.request.getPathInfo %}
     <a href="http://www.myskreen.com/les-inconnus/?follow" target="_blank" data-id="1" class="fav btn btn-suivre btn-suivre-b" data-placement="left" data-original-title="Les Inconnus">+ Suivre les Inconnus</a>
     {% else %}
     <a data-id="1" data-channel-name="Les Inconnus" class="btn btn-suivre btn-plus btn-suivre-b fav fav-channel">Suivre Les Inconnus </a>
     {% endif %}
    </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-16 ">
  <h2 class="lato isplaying-title"><strong  class="pull-left">{{ program.title|replace({ (program.episodeof.title ~ ' - '):''}) }}</strong>
      <div class="share-block">
        {% for slider_share in selection_sketches | to_array %}
          {% if loop.first %}
            {% for program_share in slider_share.programs | to_array %}
              {% if loop.first %}
                {% include 'SkreenHouseFactoryV3Bundle:Main:_share.html.twig' with {url: 'http://www.myskreen.com' ~ program_share.seo_url, text: program_share.title|replace({ (program.episodeof.title ~ ' - '):''}) ~ ' est sur @mySkreen en streaming gratuit.' } %}
              {% endif %}
            {% endfor %}
          {% endif %}
        {% endfor %} 
      </div>
    </h2>
  </div>
</div>
{% endif %}
{% endfor %}
{% endif %}
<div class="row {% if loop.index != 1 %}hide{% endif %}" data-selection-id="{{slider.id}}">
 <div class="col-xs-16 ">
  <div id="carousel-{{slider.id}}" class="vidplaylist carousel slide" data-ride="carousel" data-interval="0"> 

    <div class="btn-wrap">   
       <a class="left_b" href="#carousel-{{slider.id}}" data-slide="prev">
        <i class="glyphicon glyphicon-play"></i>
      </a>
      <a class="right_b " href="#carousel-{{slider.id}}" data-slide="next">
        <i class="glyphicon glyphicon-play"></i>
      </a>
  </div>

  <div class="carousel-inner">
    <div class="item active">
      {% for program in slider.programs | to_array %}

      <div class="vignette"  data-play-url="{{ program.player.player.url }}"  data-id="{{program.id}}" data-selection="{{slider.id}}" data-play-title="{{ program.title|replace({ (program.episodeof.title ~ ' - '):''})}}" data-share-text="{{ ('Les Inconnus / ' ~ program.title ~ ' sur @mySkreen en streaming gratuit.')|replace({ (program.episodeof.title ~ ' - '):''})|url_encode|replace({'+': '%20'})}}" data-play-seo-url="{{('http://www.myskreen.com' ~ program.seo_url)|url_encode}}">
        <div class="img-small"  style="background:url('{{ program.picture|replace({'/150/200/':'/165/165/'}) }}') top center no-repeat"></div>
        <span><mark>{{loop.index}}</mark>{{ program.title|replace({ (program.episodeof.title ~ ' - '):''}) }}</span>
      </div>
      {% if loop.index%5 == 0 and loop.index != 1 %}
    </div>
    <div class="item">
      {% endif %}
      {% endfor %}
    </div>
  </div>

</div>
</div>
</div>
{% endfor %}