{% set counts = data|pagination(page,30) %}
<ul class="pagination">
  {% for keys,count in counts%}
    {% if count != "..."%}
      <li>
        <span data-link-to-replace="{{ path('channel_page', {slug: app.request.get('slug'), page: keys})|rot13 }}">
          ...
        </span>
      </li>
    {% elseif count == true %}
        <li {% if keys == page %}class="active" {% endif %}><a href="{{ path('channel_page', {slug: app.request.get('slug'), page: keys })|rot13 }}">{{keys}}</a></li>
    {% elseif count == false%}
      <li>
        <span data-link-to-replace="{{ path('channel_page', {slug: app.request.get('slug'), page: keys})|rot13 }}">
          {{keys}}
        </span>
      </li>
    {% endif %}
  {% endfor %}
</ul>