{% if custom_header %}
{% render(controller('SkreenHouseFactoryV3Bundle:Channel:header'~ channel.id, {data:data, from_selection: from_selection, channel: channel, fav: 'page', trigger_fav: 
channel.personne is defined ? 'Suivre ' ~ data.name : ''})) %}
{% else %}
{% include  'SkreenHouseFactoryV3Bundle:Channel:_header.html.twig' with {data:data, fav: 'page', trigger_fav: channel.personne is defined ? 'Suivre ' ~ data.name : ''} %}
{% endif %}
<div id="view-page" class="container channel{{ channel.id }}">
  <div class="row">
      {% for slider in data.sliders|to_array if slider.id is defined %}
      {% if loop.index == 2%}
        {% include 'SkreenHouseFactoryV3Bundle:Slider:social-slider.html.twig' with {slider: slider, loop_index:loop.index, h_title: "h2",channel: channel} %}
      {% endif %}
        {% include 'SkreenHouseFactoryV3Bundle:Slider:_slider-new.html.twig' with {slider: slider, loop_index:loop.index, h_title: "h2",channel: channel} %}
      {% endfor %}
  </div>
  <div class="row">
      <br/>
      <br/>
      <div class="well">
      {% if channel.personne is defined and channel.personne %}
      <p class="description">{{ channel.personne.description|raw }}</p>
      {% elseif channel.text is defined and channel.text %}
      <p class="description">{{ channel.text|raw }}</p>
      {% endif %}
      </div>
  </div>
</div>
</div>
