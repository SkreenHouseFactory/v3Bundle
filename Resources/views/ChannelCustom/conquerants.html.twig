{% extends "SkreenHouseFactoryV3Bundle:Channel:_base-channel.html.twig" %}

{% block customcontent %}
  {% include  'SkreenHouseFactoryV3Bundle:Channel:_description.html.twig' %}
  <div id="cover" class="cover56">
    <div id="channel56" class="header-container">
      <a class="cover-wrap" href="/film/comedies/5108833-les-conquerants/"></a>
      <div class="header-content">
        <div class="player-wrap">
          <iframe src="http://api.myskreen.com/skPlayerPlugin/iframe.php?is_iframe=1&noPub=1&program_id=5108833&height=213&width=367&channel_header=1&fromWebsite=1" height="213" width="367"></iframe>
        </div>
        <div class="title-for-mobile">
          <h2>Les Conquérants</h2>
          <span>Un film de Xabi Molia</span>
        </div>
        <div class="wrap-follow" data-playlist="page">
          {% include 'SkreenHouseFactoryV3Bundle:Main:_btn-suivre.html.twig' with {id: channel.id, class: 'fav-page', name: channel.title, text: 'Suivre ' ~ channel.title|trans } %}
        </div>
        <div class="share-block">
          {% include 'SkreenHouseFactoryV3Bundle:Main:_share.html.twig' with {url: 'http://www.myskreen.com/' ~ channel.slug, text: 'La chaîne ' ~ channel.title ~ ' est sur @mySkreen   ' } %}
        </div>
      </div>
    </div>
  </div>
  {% include  'SkreenHouseFactoryV3Bundle:Channel:_packs.html.twig' %}
{% endblock %}

{% block custompagestyles %}
  {% stylesheets
    '@SkreenHouseFactoryV3Bundle/Resources/public/less/channel/conquerants.less'
      filter='lessphp,?yui_css'
      output='css/channel/conquerants.css'
    %}
    <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
  {% endstylesheets %}
{% endblock %}