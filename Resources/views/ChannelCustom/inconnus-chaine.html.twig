{% extends "SkreenHouseFactoryV3Bundle:Channel:_base-channel.html.twig" %}

{% block customcontent %}
  {% include  'SkreenHouseFactoryV3Bundle:Channel:_description.html.twig' %}
  <div id="cover" class="cover1">
    <div id="channel1" class="header-container">
      <div class="container">
        <div class="row" >
          <div class="col-xs-8">
          </div>
          <div class="col-xs-8">
            <iframe  class="pull-right" frameborder="0" scrolling="no" width="400" height="260" src="http://api.myskreen.com/skPlayerPlugin/iframe.php?is_iframe=1&amp;play=33505825&amp;fromWebsite=1&amp;default_version=VOST"></iframe>
          </div>
        </div>
        <div id="channel1_sponsors">
          <a id="fr2" class="liens_sponsors" href="http://www.france2.fr/" target="_blank" title="France 2"></a>
          <a id="myskreen" class="liens_sponsors" href="http://www.myskreen.com/" target="_blank" title="mySkreen.com"></a>
          <a id="venteprivee" class="liens_sponsors" href="http://fr.vente-privee.com" target="_blank" title="Vente Privée"></a>
          <a id="parisien" class="liens_sponsors" href="http://www.leparisien.fr/" target="_blank" title="Le Parisien"></a>
          <a id="rtl" class="liens_sponsors" href="http://www.rtl.fr/" target="_blank" title="RTL"></a>
        </div>
        <div class="header-content">
          <div class="share-block">
            {% include 'SkreenHouseFactoryV3Bundle:Main:_share.html.twig' with {url: 'http://www.myskreen.com/' ~ channel.slug, text: 'La chaîne ' ~ channel.title ~ ' est sur @mySkreen   ', fb_layout: 'box_count', tw_layout: 'vertical', g_size: 'tall', g_annotation: 'bubble' } %}
          </div>
          {% include 'SkreenHouseFactoryV3Bundle:Main:_btn-suivre.html.twig' with {id: channel.id, class: 'suivre-btn', fav: 'channel', name: 'Les Inconnus', text: 'Ajouter "Les Inconnus" à vos listes' } %}
          <a class="btn btn-default" href="http://www.myskreen.com/film/comedies/5088919-les-trois-freres-le-retour/">Le Film</a>
          <a class="btn btn-default" href="http://www.myskreen.com/l-integrale-des-inconnus/">L'Intégrale</a>
          <a class="btn btn-default" href="http://www.myskreen.com/les-vrais-inconnus-chaine-parodies-streaming/">Les Parodies</a>
        </div> 
      </div>
    </div>
  </div>
  {% include  'SkreenHouseFactoryV3Bundle:Channel:_packs.html.twig' %}
{% endblock %}

{% block custompagestyles %}
  {% stylesheets
    '@SkreenHouseFactoryV3Bundle/Resources/public/less/channel/inconnus-chaine.less'
      filter='lessphp,?yui_css'
      output='css/channel/inconnus-chaine.css'
    %}
    <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
  {% endstylesheets %}
{% endblock %}