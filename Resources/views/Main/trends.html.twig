{% extends 'SkreenHouseFactoryV3Bundle::layout.html.twig' %}
{# HEAD Title #}
{% block title %}
  Tendances
{% endblock %}

{# HEAD Description #}
{% block description %}
  Bla Bla Bla
{% endblock %}

{# HEAD Pagescripts #}
{% block pagescripts %}
  {% javascripts
    '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/raphael-min.js'
    '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/g.raphael-min.js'
    '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/g.line-min.js'
    '@SkreenHouseFactoryV3Bundle/Resources/public/js/scripts/trends.js'
    filter='?yui_js' 
    output='js/compiled/pages/trends.js'  %}
    <script src="{{ asset_url }}"></script>
  {% endjavascripts %}
{% endblock %}

{% block content %}
  <div class="container">
    Hello World !
    <h2>Programmes</h2>
    <div id="program"></div>
    <h2>Personnalités</h2>
    <div id="person"></div>
    <h2>Catégories</h2>
    <div id="category"></div>
    <h2>Chaînes</h2>
    <div id="channel"></div>
  </div>
{% endblock %}