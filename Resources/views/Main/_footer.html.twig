<a name="footer"></a>
<div class="container">
  <div id="boost" class="row">
    {% render 'SkreenHouseFactoryV3Bundle:Main:boost' with {}, {'standalone': true} %}
  </div>
  <hr/>
  <div class="row" style="font-size: 0.9em;">
    <div id="footer-replay" class="span2">
      <h5>Replay TV</h5> 
      <a href="{{ path('tvgrid') }}">Programme TV & Replay</a>
      <a href="{{ path('channel', {slug: 'm6'}) }}">M6 Replay</a>
      <a href="{{ path('channel', {slug: 'tf1'}) }}">TF1 Replay</a>
      <a href="{{ path('channel', {slug: 'w9'}) }}">W9 Replay</a>
      <a href="{{ path('channel', {slug: 'france-2'}) }}">France 2 Replay</a>
      <a href="{{ path('channel', {slug: 'france-3'}) }}">France 3 Replay</a>
    </div>
    <div id="footer-channels" class="span2">
      <h5>Chaînes</h5> 
      <a href="{{ path('sitemap_channels', {type: 'broadcast'}) }}">Chaînes TV & Replay</a>
      <a href="{{ path('sitemap_channels', {type: 'vod'}) }}">Chaînes Vidéo à la demande</a>
      <a href="{{ path('sitemap_channels', {type: 'webcast'}) }}">Chaînes Web</a>
      <a href="{{ path('sitemap_channels', {type: 'cinema'}) }}">Salles et horaires de cinéma</a>
      <a href="{{ path('sitemap_channels', {type: 'dvd'}) }}">Location Dvd et Blu-ray</a>
    </div>
    {% if app.request.get('home') and 
          app.request.get('home') not in ['tv-replay','cinema','video-a-la-demande','mixte'] %}
      {% set base_footer = app.request.get('home') %}
    {% else %}{% set base_footer = 'film' %}{% endif %}
    <div id="cats" class="span2">
      <h5>Catégories</h5>
      {% set footerspin =  '' %}
      {#% if base_footer in ['film'] %#}
      <a href="/film/film-culte/">Film culte{{ footerspin }}</a>
      <a href="/film/ovni-experimental/">OVNI / Expérimental{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['film','dessin-anime-et-manga','serie'] %#}
      <a href="/film/romance/">Romance{{ footerspin }}</a>
      <a href="/film/policier-thriller/">Policier - Thriller{{ footerspin }}</a>
      <a href="/film/fantastique/">Fantastique{{ footerspin }}</a>
      <a href="/film/comedies/">Comédies{{ footerspin }}</a>
      <a href="/film/drames/">Drames{{ footerspin }}</a>
      <a href="/film/jeunesse/">Jeunesse{{ footerspin }}</a>
      <a href="/film/erotique/">Erotique{{ footerspin }}</a>
      </div>
      <div id="cats-1" class="span2">
      <a href="/film/action-aventure/">Action - Aventure{{ footerspin }}</a>
      <a href="/film/epouvante-horreur/">Epouvante - horreur{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['film','documentaire'] %#}
      <a href="/film/biographie-biopic/">Biographie - Biopic{{ footerspin }}</a>
      <a href="/documentaire/societe/">Société{{ footerspin }}</a>
      <a href="/documentaire/arts/">Arts{{ footerspin }}</a>
      <a href="/documentaire/histoire/">Histoire{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['documentaire'] %#}
      <a href="/documentaire/sciences/">Sciences{{ footerspin }}</a>
      <a href="/documentaire/justice/">Justice{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['film','documentaire','serie','dessin-anime-et-manga'] %#}
      <a href="/serie/science-fiction/">Science-fiction{{ footerspin }}</a>
      <a href="/emission/decouverte/">Découverte{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['emission','journal','documentaire'] %#}
      </div>
      <div id="cats-2" class="span2">
      <a href="/emission/sport/">Sport{{ footerspin }}</a>
      <a href="/emission/sante-bien-etre/">Santé - bien-être{{ footerspin }}</a>
      <a href="/emission/loisirs-passions/">Loisirs - Passions{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['emission','journal'] %#}
      <a href="/emission/divertissement-humour/">Divertissement-humour{{ footerspin }}</a>
      <a href="/emission/archives/">Archives{{ footerspin }}</a>
      {#% endif %}
      {% if base_footer in ['serie'] %#}
      <a href="/serie/series-americaines/">Séries américaines{{ footerspin }}</a>
      <a href="/serie/series-francaise/">Séries françaises{{ footerspin }}</a>
      <a href="/serie/serie-animee/">Série animée{{ footerspin }}</a>
      {#% endif %#}
      <hr/>
      <a href="/partenaires">Partenaires</a>
      <a href="http://www.videofutur.fr" target="_blank">Location DVD, Blu-ray</a>
    </div>
  </div>
  <hr/>
