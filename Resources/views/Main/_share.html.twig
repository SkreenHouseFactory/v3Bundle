<div class="fb-container">
  <div class="fb-like" style="z-index: 16; margin-left: 0;" data-share="false" data-href="{{url}}" data-layout="{{ fb_layout is defined ? fb_layout : 'button_count' }}" data-action="like" data-show-faces="false" data-share="true"></div>
</div>
<iframe class="pull-left twitter-share" style="height: 65px; width: 70px; margin-right:0px; margin-left: 50px;" allowtransparency="true" frameborder="0" scrolling="no" src="http://platform.twitter.com/widgets/tweet_button.1355514129.html#_=1356719798951&amp;id=twitter-widget-0&amp;lang=fr&amp;original_referer={{ url|url_encode }}&amp;size=m{{ tw_layout is defined ? '&count=' ~ tw_layout : '' }}&amp;text={{ text|url_encode|replace({'+': '%20'}) }}&amp;url={{ url|url_encode }}" class="block-twitter" data-twttr-rendered="true"></iframe>
<iframe  class="g-plus-share pull-right" frameborder="0" scrolling="no" style="height: 65px; border-style:none; margin: 0; visibility:visible; width:60px;" id="I0_share" name="I0_share" src="https://plusone.google.com/_/+1/fastbutton?bsv&amp;size={{ g_size is defined ? g_size : 'medium' }}{{ g_annotation is defined ? '&annotation=' ~ g_annotation : '' }}&amp;hl=fr&amp;url={{ url|url_encode }}&amp;id=I0_share" allowtransparency="true" data-gapiattached="true" title="+1"></iframe>