<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

	<title>{% block title endblock %} | {{ 'service.name'|trans }}</title>
	<meta name="description" content="{% block description %}{% endblock %}">
	<meta name="author" content="mySkreen">
	<meta name="language" content="fr-FR" />

	<meta name="viewport" content="width=device-width">

	<link rel="stylesheet/less" href="{{ asset('bundles/skreenhousefactoryv3/initializr/less/style.less') }}">
	<script src="{{ asset('bundles/skreenhousefactoryv3/initializr/js/libs/less-1.3.0.min.js') }}"></script>
	
	<!-- Use SimpLESS (Win/Linux/Mac) or LESS.app (Mac) to compile your .less files
	to style.css, and replace the 2 lines above by this one:

	<link rel="stylesheet" href="{{ asset('bundles/skreenhousefactoryv3/initializr/less/style.css') }}">
	 -->

	<script src="{{ asset('bundles/skreenhousefactoryv3/initializr/js/libs/modernizr-2.5.3-respond-1.1.0.min.js') }}"></script>
</head>
<body>
<!--[if lt IE 7]><p class=chromeframe>Votre navigateur est <em>ancien!</em> <a href="http://browsehappy.com/">Mettez-le à jour</a> ou <a href="http://www.google.com/chromeframe/?redirect=true">installez Google Chrome Frame</a> pour profiter utiliser mySkreen dans les meilleurs conditions.</p><![endif]-->

    <div class="modal hide" id="skModal">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">×</button>
        <h3>{{ 'loading'|trans }} ...</h3>
      </div>
      <div class="modal-body">
      </div>
      <div class="modal-footer">
      </div>
    </div>

    <div id="header">
    {% include 'SkreenHouseFactoryV3Bundle:Main:_header.html.twig' with {'menus': menus} %}
    </div>

    {% block content %}
    {% endblock %}

    <div id="player">
      <i class="icon-resize-small icon-white"></i>
      <div class="metas player-overlay"><span class="title"></span><br/><span class="subtitle"></span><br/><span class="actions"></span></div>
      <div class="embed"></div>
    </div>

    <div id="redirect" class="collapse in">
    {% block redirect %}
    {% endblock %}
    </div>
    
    <div id="top-links">
    {% for key,m in menus %}
      <ul>{{  'all.the.format'|trans({'%format%': key|lower}) }} : 
      {% for l in m %}
        <li><a href="link-v2('{{ l.seo_url }}');" title="{{ l.name }} {{  key }} {{ 'access.streaming'|trans }}">{{ l.name }}</a></li>
      {% endfor %}
      </ul>
    {% endfor %}
    </div>

    <div class="container">

    {#
      <footer>
        <hr />
        <p>&copy; The Skreen House Factory 2012</p>
      </footer>
    #}

    </div> <!-- /container -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ asset('bundles/skreenhousefactoryv3/initializr/js/libs/jquery-1.7.2.min.js') }}"><\/script>')</script>
<script src="{{ asset('bundles/skreenhousefactoryv3/initializr/js/libs/bootstrap/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/initializr/js/libs/bootstrap/ajax-typeahead.js') }}"></script>

<script src="{{ asset('bundles/skreenhousefactoryv3/js/vendors/jquery.cookies.min.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/js/vendors/easyXDM.min.js') }}"></script>

<script src="{{ asset('bundles/skreenhousefactoryv3/js/api.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/js/slider.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/js/ui.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/js/session.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/js/player.js') }}"></script>
<script src="{{ asset('bundles/skreenhousefactoryv3/js/script.js') }}"></script>
<script>
	var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
	(function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
	g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
	s.parentNode.insertBefore(g,s)}(document,'script'));
</script>

</body>
</html>