  var c_name = 'myskreen_intersitiel_mobile'; var c_value = 'displayed'; var c_exdays = 1;
  function checkInterstitielMobileVersion(){
    var i,x,y,ARRcookies=document.cookie.split(";");
    for (i=0;i<ARRcookies.length;i++){
      x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("=")); y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
      x=x.replace(/^\s+|\s+$/g,"");
      if (x==c_name && unescape(y) == c_value) { return true; }
      //alert('cookie: ' + x + '=' + y);
    }
    return false;
  }
  interstitiel_mobile = false;
  if (mobilecheck() && !checkInterstitielMobileVersion()) {
    interstitiel_mobile = true;
  }
  //interstitiel_mobile = true;
  //console.log('mobilecheck():'+mobilecheck(), 'checkInterstitielMobileVersion()'+checkInterstitielMobileVersion(), 'interstitiel_mobile:'+interstitiel_mobile, 'c_name:'+c_name);
  window.onload = function(){
    if (interstitiel_mobile == true) {
      //console.log('set mobile interstitiel');
      document.cookie=c_name + '=' + c_value;
      $('#interstitiel-mobile').show();
      $('#interstitiel-mobile span').on('click', function(){
        $('#interstitiel-mobile').remove();
      });
      setTimeout(function(){
        if (!$('#interstitiel-mobile iframe').length) {
          $('#interstitiel-mobile').remove();
        }
      }, 3000)
    } else {
      $('#interstitiel-mobile').remove();
    }
  }
