<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="fr"> <![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie9 lt-ie8" lang="fr"> <![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie9" lang="fr"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="fr"> <!--<![endif]-->
<head>
  {% spaceless %}

  <title>{% block title endblock %} | mySkreen.com</title>
  <meta name="description" content="{% block description %}{% endblock %}">
  {#
  <script>
  var c_name = 'myskreen_mobile'; var c_value = 'deny'; var c_exdays = 30;
  function checkDenyMobileVersion(){
    var i,x,y,ARRcookies=document.cookie.split(";");
    for (i=0;i<ARRcookies.length;i++){
      x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("=")); y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
      x=x.replace(/^\s+|\s+$/g,"");
      if (x==c_name && unescape(y) == c_value) { return true; }
      //alert('cookie: ' + x + '=' + y);
    }
    return false;
  }
  //set deny
  if (window.location.href.indexOf('setDenyMobile') != -1) {
    var exdate=new Date();exdate.setDate(exdate.getDate() + c_exdays);
    var c_value=escape(c_value) + ((c_exdays==null) ? '' : '; expires='+exdate.toUTCString());
    document.cookie=c_name + '=' + c_value;
  // -> m
  } else {
    if (!checkDenyMobileVersion()) {
      var navl = document.location.href;
      var navt = 'Blackberry,Blazer,Elaine,Handspring,HTC,IEMobile,iPhone,iPod,Kyocera,LG,MIDP,Motorola,NetFront,Nokia,Palm,PlayStation Portable,Samsung,Smartphone,SonyEricsson,Symbian,UP.Browser,WAP,Windows CE,GT-'.split(','), nava = navigator.userAgent;
      for (e = 0; e < navt.length; e++) if (nava.indexOf(navt[e]) != -1) { if (navl.match(/typhon\./)) { window.location.href = '/app_dev.php/m/route/' } else { window.location.href = navl.match(/preprod\./) ? '/m/route/' : 'http://m.myskreen.com/m/route/'}};
    }
  }
  </script>
  #}
  <meta name="author" content="SkreenHouse Factory">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta charset="UTF-8">
  <meta name="language" content="fr-FR" />
  <meta name="revisit-after" content="2 days" />
  <meta property="og:locale:alternate" content="fr_FR" />
  <meta property="og:site_name" content="mySkreen.com" />
  <meta property="fb:app_id" content="{% if app.environment != 'prod' %}101348563248431{% else %}193945860617344{% endif %}" />
  <meta name="msvalidate.01" content="578206D7F79F12C1D7F845473C11F955" />
  <meta name="verify-a" value="e548aa1414e42be0692f">
  <meta name="google-site-verification" content="XrGdiUEwEtqEFnxru9FgykHR9mmSCX6O1hky_g9gTDU" />
  {% if app.request.get('force_metatags') is null and (app.environment != 'prod' or 'preprod' in app.request.host or 'typhon' in app.request.host) %}
  <meta name="robots" content="noindex,nofollow">
  {% else %}
  {% block meta endblock %}
  {% endif %}
  {% if app.request.pathinfo != app.request.requestUri %}
  
  <link rel="canonical" href="http://www.myskreen.com{{ app.request.pathinfo }}" />
  {% endif %}
  <link rel="author" href="https://plus.google.com/106555719800030017985" />
  <meta name="apple-mobile-web-app-capable" content="yes" />
  <link rel="search" type="application/opensearchdescription+xml" href="http://www.myskreen.com/opensearch.xml" title="Recherche de vidéos mySkreen"/>
  <link rel="apple-touch-icon" href="{{ asset('bundles/skreenhousefactoryv3/images/icons/iphone.png') }}" />
  <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/skreenhousefactoryv3/images/icons/ipad.png') }}" />
  <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('bundles/skreenhousefactoryv3/images/icons/iphone-retina.png') }}" />
  <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('bundles/skreenhousefactoryv3/images/icons/ipad-retina.png') }}" />
  <link rel="apple-touch-startup-image" href="{{ asset('bundles/skreenhousefactoryv3/images/icons/startup.png') }}" />
  <meta name="viewport" content="width=1010, maximum-scale=1, user-scalable = yes" />
  {% stylesheets
  '@SkreenHouseFactoryV3Bundle/Resources/public/less/bootstrap-overload.less'
  '@SkreenHouseFactoryTvBundle/Resources/public/less/couchmode.less'
  '@SkreenHouseFactoryPopinBundle/Resources/public/less/popin.less'
  '@SkreenHouseFactoryV3Bundle/Resources/public/less/modal.less'
  '@SkreenHouseFactoryV3Bundle/Resources/public/less/_default.less'
  filter='lessphp'
  output='css/v3.css'
  %}
  <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
  {% endstylesheets %}
</head>
{% endspaceless %}
<body class="{% if block('body_class') %}{% block body_class %}{% endblock %}{% else %}view-{{ app.request.attributes.get('_route') }}{% endif %}"{% if block('body_background') %} style="background:{% block body_background %}{% endblock %};"{% endif %}{% if app.environment == 'dev' %} data-env="{{ app.request.host|split('.')|slice(1,1)|join('') }}"{% endif %}>

  {% spaceless %}
  <!--[if lt IE 7]><p class=chromeframe>Votre navigateur est <em>ancien!</em> <a href="http://browsehappy.com/">Mettez-le à jour</a> ou <a href="http://www.google.com/chromeframe/?redirect=true">installez Google Chrome Frame</a> pour profiter utiliser mySkreen dans les meilleurs conditions.</p><![endif]-->
  <article>
    <div id="main">
      <div id="content" class="uiview-update">
        {% block content %}
        {% endblock %}
      </div>
      <div id="redirect" class="collapse in">
        {% block redirect %}
        {% endblock %}
      </div>
    </div>
  </article>

  {% render(controller('SkreenHouseFactoryV3Bundle:Main:header', {'q':app.request.get('q') ? app.request.get('q') : null })) %}
  {#% include 'SkreenHouseFactoryV3Bundle:Main:_header-nav.html.twig' %#}
  {% block header_filters %}
  {% endblock %}


  <footer>
    <div id="footer">
      {% render(controller('SkreenHouseFactoryV3Bundle:Main:footer', {})) %}
    </div>
  </footer>
  {% include 'SkreenHouseFactoryTvBundle:Main:_couchmode.html.twig' %}
  {% include 'SkreenHouseFactoryV3Bundle:Main:_modal.html.twig' %}
  <div id="fb-root"></div>
  <span class="hide">généré le {{ 'now'|date(' d/m à H:i:s') }}</span>

  {% if block('noscripts') is empty %}
  {% javascripts
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/jquery-1.10.2.min.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/jquery.cookies.min.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/bootstrap/dist/js/bootstrap.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/bootstrap-ajax-typeahead.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/modernizr.min.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/inheritance.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/flowplayer-3.2.11.min.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/flowplayer.controls-3.2.10.min.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/flowplayer.ipad-3.2.11.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/jquery.debounce-1.0.5.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/jquery.serializefullarray.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/core/*'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/session.js'
  '@SkreenHouseFactoryTvBundle/Resources/public/js/couchmode.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/ui.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/scripts/_default.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/scripts/core/*'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/scripts/playlists.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/scripts/cinema.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/scripts/skin.js'
  '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/respond.min.js'

  filter='?yui_js' 
  output='js/compiled/v3.js'  %}
  <script src="{{ asset_url }}"></script>
  <script type="text/javascript" src="https://apis.google.com/js/plusone.js">
  {lang: 'fr'}
</script>
  {% endjavascripts %}
  {% endif %}
  {% block pagescripts %}
  {% endblock %}

  {% if app.environment == 'prod' and block('nostats') is empty %}
  {% include 'SkreenHouseFactoryV3Bundle::_ga.html.twig' with {tag: 'UA-6151896-14'} %}
  {% include 'SkreenHouseFactoryV3Bundle::_chartbeat.html.twig' %}
  <script type="text/javascript">
    _gaq.push(['_setAccount', 'UA-6151896-8']);
    _gaq.push(['_trackPageview']);
  </script>
  {% endif %}
  {% endspaceless %}

</body>
</html>